    "suggest": {
        "sonata-project/doctrine-orm-admin-bundle": "~2.2"
    },
    "autoload": {
        "psr-0": { "Sonata\\CommentBundle": "" }
    },
    "target-dir": "Sonata/CommentBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    }
